{
  "presets": [
    "env",
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}